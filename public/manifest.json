{
  "manifest_version": 2,

  "name": "Demo React-Chrome extension",
  "description": "This extension shows how to run a React app as a Chrome extension",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html"
  },
  "incognito": "split",
  "permissions": [
    "https://www.google.com/",
    "background",
    "browsingData",
    "cookies",
    "geolocation",
    "history",
    "identity",
    "sessions",
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation"
  ],
  "background": {
    "pages": "background.html",
    "scripts": ["background.js"],
    "persistent": false
  }
}

